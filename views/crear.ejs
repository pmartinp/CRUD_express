<%- include("template/cabecera", {tituloWeb: "Crear Coche"}) %>

<div class="container">
    <h2>Crear nuevo Coche</h2>
    <form action="/coches" class="my-2" method="POST">
        <input type="number" placeholder="Id" class="form-control my-2" name="id">
        <input type="text" placeholder="Marca" class="form-control my-2" name="marca">
        <input type="text" placeholder="Modelo" class="form-control my-2" name="modelo">
        <input type="number" placeholder="Potencia" class="form-control my-2" name="potencia">
        <button type="submit" class="btn btn-dark btn-block">Agregar</button>
    </form>
</div>

<%- include("template/footer") %>